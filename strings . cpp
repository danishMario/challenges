#include <iostream>
#include <string>
using namespace std;

int main() {
	// Complete the program
    string a , b;
    cin >> a;
    cin >> b;
    int leng = a.size();
    int leng0 = b.size();
    cout << leng << " " << leng0 << endl;
    string c = a + b;
    cout << c << endl;
    swap (a[0] , b[0]);
    cout << a << ' ' << b << endl;
  
    return 0;
}
